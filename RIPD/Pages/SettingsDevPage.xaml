<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RIPD.Pages.SettingsDevPage"
             Title="Dev Settings">
  <VerticalStackLayout>
    <Label Text="{Binding Protocol}"/>
    <Label Text="{Binding Domain}"/>
    <Label Text="{Binding Port}"/>
    <Label Text="{Binding ApiPage}"/>
    <Label Text="{Binding StatusPage}"/>
  </VerticalStackLayout>
</ContentPage>
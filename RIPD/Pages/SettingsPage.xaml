<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewmodels="clr-namespace:RIPD.ViewModels"
             x:Class="RIPD.Pages.SettingsPage"
             x:DataType="viewmodels:SettingsVM">
  <VerticalStackLayout>
    <Button Text="Dev Settings" Command="{Binding GoToSettingsDevCommand}"/>
    <Button Text="Register" Command="{Binding GoToRegisterPageCommand}"/>
    <Button Text="Login" Command="{Binding GoToLoginPageCommand}"/>
  </VerticalStackLayout>
</ContentPage>
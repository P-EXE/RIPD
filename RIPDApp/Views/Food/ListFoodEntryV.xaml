<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:m="clr-namespace:RIPDShared.Models;assembly=RIPDShared"
             x:Class="RIPDApp.Views.ListFoodEntryV"
             x:DataType="m:DiaryEntry_Food">
  <HorizontalStackLayout>
    <VerticalStackLayout>
      <Label Text="{Binding Food.Name}"/>
      <Label Text="{Binding Food.Manufacturer}"/>
    </VerticalStackLayout>
    <Label Text="{Binding Amount}"/>
    <Label Text="{Binding Acted}"/>
  </HorizontalStackLayout>
</ContentView>

<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewmodels="clr-namespace:RIPDApp.ViewModels"
             x:Class="RIPDApp.Views.StatusBarV"
             x:DataType="viewmodels:StatusBarVM">
  <VerticalStackLayout>
    <Label Text="{Binding InternetStatus}"/>
    <Label Text="{Binding User.UserName}"/>
  </VerticalStackLayout>
</ContentView>
